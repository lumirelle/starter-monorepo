{
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "bun -F '*' dev",
    "build": "bun -F '*' build",
    "start": "bun -F pkg-placeholder start",
    "docs": "bun -F docs docs:dev",
    "docs:build": "bun -F docs docs:build",
    "docs:preview": "bun -F docs docs:preview",
    "play": "bun -F playground play",
    "lint": "eslint --cache .",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "prerelease": "bun test && bun typecheck",
    "release": "bumpp -r"
  },
  "workspaces": {
    "packages": [
      "docs",
      "playground",
      "packages/*",
      "examples/*"
    ],
    "catalogs": {
      "dev": {
        "@antfu/eslint-config": "^6.2.0",
        "bumpp": "^10.3.1",
        "czg": "^1.12.0",
        "eslint": "^9.39.1",
        "eslint-plugin-format": "^1.0.2",
        "lint-staged": "^16.2.7",
        "simple-git-hooks": "git+https://github.com/toplenboren/simple-git-hooks.git#7625657",
        "stylelint": "^16.26.0",
        "taze": "^19.9.0",
        "tsdown": "^0.17.0",
        "typescript": "^5.9.3",
        "vite": "^7.2.4"
      },
      "docs": {
        "@iconify-json/svg-spinners": "^1.2.4",
        "@shikijs/vitepress-twoslash": "^3.15.0",
        "@unocss/reset": "^66.5.9",
        "@vueuse/core": "^14.0.0",
        "floating-vue": "^5.2.2",
        "pinia": "^3.0.4",
        "unocss": "^66.5.9",
        "unplugin-vue-components": "^30.0.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitepress": "^2.0.0-alpha.15",
        "vitepress-plugin-group-icons": "^1.6.5",
        "vue": "^3.5.25"
      },
      "inlined": {
        "@antfu/utils": "^9.3.0"
      },
      "testing": {
        "vitest-package-exports": "^0.1.1",
        "yaml": "^2.8.2"
      },
      "types": {
        "@types/bun": "^1.3.3"
      }
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:dev",
    "@antfu/utils": "catalog:inlined",
    "@types/bun": "catalog:types",
    "bumpp": "catalog:dev",
    "czg": "catalog:dev",
    "eslint": "catalog:dev",
    "eslint-plugin-format": "catalog:dev",
    "lint-staged": "catalog:dev",
    "pkg-placeholder": "workspace:*",
    "simple-git-hooks": "catalog:dev",
    "stylelint": "catalog:dev",
    "taze": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vitest-package-exports": "catalog:testing",
    "yaml": "catalog:testing"
  },
  "simple-git-hooks": {
    "pre-commit": "bun lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
